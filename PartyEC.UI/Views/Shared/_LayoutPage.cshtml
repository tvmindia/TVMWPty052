<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Online Party Shopping in Qatar. Venue, Cake, Flowers &amp; More</title>
    <meta name="description" content="Find the best party vendors in Qatar. Organise a party online. Book Venue &amp; Photographer within your budget. Order cake, chocolates, gifts &amp; flowers online.">
    <meta name="keywords" content="Party Planner, Party Halls In Qatar, Birthday Party Venues, Events In Qatar, Order Cake online in Qatar, Top Quality Cakes online, Order Customised Cake, Personalised Cake online,  Flowers online,  Chocolate online, Cake free home delivery, Free Home delivery, Gift online in Qatar, Book your favourite Venue, Birthday venue, Birthday Cake, Birthday photographer, Doha Selfie Photography, Wedding Anniversary Photography, Wedding Anniversary location. ">
    <meta name="robots" content="INDEX,FOLLOW">
    <link rel="icon" href="https://www.partyec.com/media/favicon/default/favicon_3.png" type="image/x-icon">
    <link rel="shortcut icon" href="https://www.partyec.com/media/favicon/default/favicon_3.png" type="image/x-icon">
    @*<meta charset="utf-8">*@
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*<meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>*@

    <!-- Bootstrap Core CSS -->
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600,800,700,900' rel='stylesheet' type='text/css'>
    <style>
        #loading-img {
    background: url('../../Content/images/Loader.gif') center center no-repeat;
    height: 100%;
    z-index: 1040;
}
.mega-header {
    text-align: center;
    position: relative;
}
.overlayLoad {
    background: #faf8f8;
    display: none;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    opacity: 0.5;
    z-index: 1040;
}

 #block_loginredirect{
      width: 845px;
      height: 384px;
      margin: 72px auto 0;
      -moz-border-radius: 4px;
      -webkit-border-radius: 4px;
      border-radius: 4px;
      background: #fff url(http://www.ebpaidrev.com/systemerror/block.gif) no-repeat 0 51px;
  }
  #block_loginredirect div{
      padding: 100px 40px 0 186px;
  }
  #block_loginredirect div h2{
      color: #218bdc;
      font-size: 24px;
      display: block;
      padding: 0 0 14px 0;
      border-bottom: 1px solid #cccccc;
      margin-bottom: 12px;
      font-weight: normal;
  }



    </style>


</head>
<body>
    <nav class="navbar navbar-default">
        
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#" style="border-radius:50%"><img alt="PartyEC" src=@Url.Content("~/Content/images/partyEClogo.png")></a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <br />
                
                @Html.Action("_MenuNavBar", "DynamicUI")
              
                <ul class="nav navbar-nav navbar-right">
                    <li role="presentation" class="dropdown">
                        <a href="/Order">
                            <i class="fa fa-envelope-o"></i>
                            @*<span class="badge bg-green">6</span>*@
                        </a>
                    </li>
                    <li role="presentation" class="dropdown">
                        <a href="/Reviews" >
                            <i class="fa fa-truck"></i>
                            @*<span class="badge bg-green">6</span>*@
                        </a>
                    </li>
                   @*<li><a href="#"><strong>@User.Identity.Name</strong></a></li>*@
                  
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><strong>@User.Identity.Name</strong><span class="caret"></span></a>
                        <ul class="dropdown-menu">
                          
                            @*<li role="separator" class="divider"></li>*@
                            <li><a href="/Account/Logout"><span class="glyphicon glyphicon-log-out"></span> Logout</a></li>
                        </ul>
                    </li>

                </ul>
            </div>
       
        @*@Html.Action("_MenuNavBar", "DynamicUI")*@
        
        @*@Html.Action("TopNav", "Home")*@     @*Navbar rendering*@
        @*@Html.Partial("TopNav")*@
        @*@Html.RenderPartial("TopNav")*@


    </nav>

    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-home"></i></a><span class="divider"> »</span></li>
        <li><a href="#">Catalog</a><span class="divider"> »</span></li>
        <li class="active">@ViewBag.PageName</li>
    </ol>
        <div class="container-fluid">
            @RenderBody()
         
        </div>
    
       <div class="overlayLoad" id="displaywait">
        <!------ loading --->
        <div id="loading-img"></div>
        </div>
    
    <nav class="navbar navbar-default">
        
            <footer>
                <p>&copy;Thrithvam Technologies @DateTime.Now.Year</p>
            </footer>
       
    </nav>
  
    <!-- Modal -->
    <div class="modal fade" id="RedirectToLoginModel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Warning</h4>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div id="block_loginredirect">
                            <div>
                                <h2>Your session has been expired please login..!</h2>
                               
                                <p>
                                    If you are the owner of the account and are regularly seeing this error, please read more about it in our <a href="http://www.partyec.com/support/">knowledgebase</a>.
                                    If you have any questions, please contact our Technical support team.
                                </p>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button onclick="RedirectToLogin()" class="btn btn-lg btn-success" type="submit">LOGIN</button>
                    @*<a href="/Account/Logout" class="btn btn-primary" value="Login"/>*@
               </div>
            </div>
        </div>
    </div>
    

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/bootstrapdatepicker")
        @Scripts.Render("~/bundles/userpluginjs")
        @RenderSection("scripts", required: false)
       <script type="text/javascript">
        function RedirectToLogin()
        {
            var appAddress = window.location.protocol + "//" + window.location.host + "/";
            window.location = appAddress + "/Account/Logout/";
        }
        </script>

</body>

</html>
